network:
  tunnels:
    provider0:
      mode: vxlan
      id: 16
      local: {{ configs[inventory_hostname].local_ip }}
{% for remote_ip in configs[inventory_hostname].remote_ips %}
      remote: {{ remote_ip }}
{% endfor %}
      addresses: [ {{ configs[inventory_hostname].tunnel_ip }} ]
      mac-learning: true
  version: 2
